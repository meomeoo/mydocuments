<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>model.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Userbean</servlet-name>
        <servlet-class>com.mycompany.login.resources.Userbean</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>model.NewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserBean</servlet-name>
        <servlet-class>Controller.UserBean</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Userbean</servlet-name>
        <url-pattern>/Userbean</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserBean</servlet-name>
        <url-pattern>/UserBean</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
